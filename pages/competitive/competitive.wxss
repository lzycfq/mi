/* pages/competitive/competitive.wxss */
page{
  background: white;
}

.competitive_tuijian{
  width: 750rpx;
  height: 300rpx;
}
.competitive_tuijian image{
  width: 100%;
  height: 100%;
}
.ShopNavsecond {
background: white;
	float: left;
	width: 100%;

border-bottom: 15rpx #ebebeb solid;

}
 .ShopNavsecond image {
	height: 64rpx;
  width: 84rpx;
}

 .ShopNavsecond view {
	width:20%;
	text-decoration: none;
	height: 140rpx;
	text-align: center;
	float: left;
	margin-top:15rpx;
}
.ShopNavsecond view span {
	width: 100%;
	float: left;
	position: relative;
  line-height: 20rpx;
	font-size: 24rpx;
	color:#888888;
	
	font-family: "Microsoft YaHei", Arial, Helvetica, sans-serif;
}
/* list */
.competitive_list{
  width: 700rpx;
  height: 100%;
  margin: 0 auto;

 
}
.competitive_list_header{
  font-size: 30rpx;
}
.competitive_list_fubiaoti{
  width: 100%;
}
.competitive_list_fubiaoti .left{
  font-size: 24rpx;
  color: #999999;
  float: left;

}
.competitive_list_fubiaoti .right{
  font-size: 24rpx;
  color: #999999;
  float: right;
}
.competitive_list_fubiaoti .right image{
  width: 38rpx;
  height: 38rpx;
  vertical-align: middle;
}

.list{
  width: 100%;
  height: 300rpx;
  margin-bottom: 20rpx;
 margin-top: 20rpx;
}

.list .list_left{
  width: 45%;
  height:300rpx;
  background: #f6f6f6;
  overflow: hidden;
  line-height: 300rpx;
  align-items: center;
  text-align: center;
  border-radius: 10%;
  float: left;
}
.list .list_left view {
position: relative;
top: 60rpx;
}
.list .list_left image{
  width: 250rpx;
  height: 170rpx;
   line-height: 300rpx;

}
.list .list_right{
  
  position: relative;
  top: 20rpx;
  width: 50%;
  float: right;
  height: 250rpx;
  
}
.list_right_header{
  overflow: hidden;
    font-size: 30rpx;
}
.list_right_jieshao{
  font-size: 25rpx;
  color: #999999;
}
.list_right_manjian{
  padding: 5rpx 10rpx 5rpx;
  border: #ff6700 2rpx solid;
  font-size:23rpx;
  color: #ff6700; 
}
.lrl{
  float: left;
  color: #ff6700;
  
}
.lrl_yuanjia{
  font-size: 25rpx;
  color: #999999;
  text-decoration: line-through;
}
.lrr{
  width: 64rpx;
  height: 64rpx;
  float: right;
}
.lrr image{
  width: 100%;
  height: 100%;
}
