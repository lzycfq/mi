/**app.wxss**/
@import "./lib/weui.wxss";

page{
  background:#f8f8f8;
  -webkit-font-smoothing: antialiased;
  box-sizing: border-box;
}
.container {
  display: flex;
  flex-direction: column;
  height: 100%;
  width: 100%; 
  box-sizing: border-box;
} 
/*隐藏滚动条*/
::-webkit-scrollbar{    
  height: 0;
  width: 0;
  color: transparent;
}
